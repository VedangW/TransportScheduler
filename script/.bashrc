CONFIG_FILE=./home.conf 

. $HOME/.asdf/asdf.sh

. $HOME/.asdf/completions/asdf.bash

# erlang binaries
alias erl="$HOME/.asdf/installs/erlang/19.0/bin/erl"
alias erlc="$HOME/.asdf/installs/erlang/19.0/bin/erlc"
alias ct_run="$HOME/.asdf/installs/erlang/19.0/bin/ct_run"
alias dialyzer="$HOME/.asdf/installs/erlang/19.0/bin/dialyzer"
alias epmd="$HOME/.asdf/installs/erlang/19.0/bin/epmd"
alias escript="$HOME/.asdf/installs/erlang/19.0/bin/escript"
alias run_erl="$HOME/.asdf/installs/erlang/19.0/bin/run_erl"
alias typer="$HOME/.asdf/installs/erlang/19.0/bin/typer"

# elixir binaries
alias elixir="$HOME/.asdf/installs/elixir/1.3.4/bin/elixir"
alias elixirc="$HOME/.asdf/installs/elixir/1.3.4/bin/elixirc"
alias iex="$HOME/.asdf/installs/elixir/1.3.4/bin/iex"
alias mix="$HOME/.asdf/installs/elixir/1.3.4/bin/mix"
